{
  "name": "gatsby-wp-starter",
  "private": true,
  "description": "A simple starter to get up and developing Gatsby theme with WordPress quickly",
  "version": "0.1.0",
  "author": "Sukjoon Kim <sjoonk@gmail.com>",
  "dependencies": {
    "gatsby": "^2.24.47",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-manifest": "^2.4.23",
    "gatsby-plugin-offline": "^3.2.23",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-source-filesystem": "^2.3.24",
